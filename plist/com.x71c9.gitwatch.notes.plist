<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>EnvironmentVariables</key>
	<dict>
		<key>PATH</key>
		<string>/opt/homebrew/bin:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin</string>
	</dict>
	<key>Label</key>
	<string>com.x71c9.gitwatch.notes</string>
	<key>LaunchOnlyOnce</key>
	<true/>
	<key>ProgramArguments</key>
	<array>
		<string>/opt/homebrew/bin/gitwatch</string>
		<string>-l</string>
		<string>1</string>
		<string>-m</string>
		<string>"Auto-commit on %d"</string>
		<string>-s</string>
		<string>60</string>
		<string>-r</string>
		<string>origin</string>
		<string>-b</string>
		<string>master</string>
		<string>/Users/x71c9/repos/notes/</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>StandardErrorPath</key>
	<string>/tmp/com.x71c9.gitwatch.notes</string>
	<key>StandardOutPath</key>
	<string>/tmp/com.x71c9.gitwatch.notes</string>
</dict>
</plist>
